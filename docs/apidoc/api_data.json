  {
    "type": "GET",
    "url": "/api/v1/task/:task_id/info",
    "title": "获取某一个任务的信息",
    "version": "1.0.0",
    "group": "____",
    "description": "<p>根据任务 id 获取某一个任务的信息</p>",
    "parameter": {
      "fields": {
        "URL Params": [
          {
            "group": "URL Params",
            "type": "String",
            "optional": false,
            "field": "task_id",
            "description": "<p>任务 id</p>"
          }
        ],
        "GET Params": [
          {
            "group": "GET Params",
            "type": "Number",
            "optional": false,
            "field": "_",
            "description": "<p>时间戳，utc 时间秒数</p>"
          }
        ]
      }
    },
    "success": {
      "fields": {
        "Success 200": [
          {
            "group": "Success 200",
            "type": "String",
            "allowedValues": [
              "\"PENDING\"",
              "\"STARTED\"",
              "\"RETRY\"",
              "\"FAILURE\"",
              "\"SUCCESS\""
            ],
            "optional": false,
            "field": "status",
            "description": "<p>任务状态</p>"
          },
          {
            "group": "Success 200",
            "type": "Object",
            "optional": false,
            "field": "info",
            "description": "<p>任务返回结果，可能是任意值</p>"
          }
        ]
      },
      "examples": [
        {
          "title": "Success-Response:",
          "content": "HTTP/1.1 200 OK\n{\n    \"code\": 0,\n    \"msg\": \"OK\",\n    \"data\": {\n        \"status\": \"SUCCESS\",\n        \"info: {\n            \"aa\": 1,\n            \"bb\": 1,\n            \"cc\": 1,\n            \"dd\": 1\n        }\n    }\n}",
          "type": "json"
        }
      ]
    },
    "filename": "apis/manger.py",
    "groupTitle": "____",
    "name": "GetApiV1TaskTask_idInfo"
  },
