          {
            "group": "Success 200",
            "type": "String",
            "allowedValues": [
              "\"PENDING\"",
              "\"STARTED\"",
              "\"RETRY\"",
              "\"FAILURE\"",
              "\"SUCCESS\""
            ],
            "optional": false,
            "field": "status",
            "description": "<p>任务状态</p>"
          },
          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "task_id",
            "description": "<p>任务 id，真正有用的 id 是这个</p>"
